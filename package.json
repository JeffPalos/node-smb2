{
  "name": "@jeffpalos/node-smb2",
  "description": "A SMB2 implementation in TypeScript (Fork)",
  "repository": "https://github.com/JeffPalos/node-smb2",
  "bugs": "https://github.com/JeffPalos/node-smb2/issues",
  "version": "1.5.2",
  "author": "Jeff Palos <jeff@becom.paris>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r dotenv/config demo/index.ts",
    "dev": "nodemon",
    "build": "tsc --build",
    "lint": "tslint --project .",
    "demo:quick": "ts-node -r dotenv/config demo/quickTest.ts",
    "demo:tree": "ts-node -r dotenv/config demo/recursiveTreeUsage.ts",
    "demo:debug": "ts-node -r dotenv/config demo/testRecursive.ts",
    "demo:original": "ts-node -r dotenv/config demo/runClient.ts",
    "test:env": "ts-node -r dotenv/config demo/testEnv.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "smb2",
    "smb",
    "cifs",
    "samba",
    "server-message-block",
    "windows",
    "file-sharing"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "des.js": "^1.1.0",
    "dotenv": "^17.2.3",
    "js-md4": "^0.3.2",
    "moment-timezone": "^0.5.31"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^13.7.4",
    "nodemon": "^2.0.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.0",
    "typescript": "^4.0.3"
  }
}
